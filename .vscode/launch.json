  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/HawkEye/bin/Debug/net6.0/HawkEye.dll",
      "args": [],
      "cwd": "${workspaceFolder}/HawkEye/bin/Debug/net6.0/",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": ".NET Core Test",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": ["test"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
